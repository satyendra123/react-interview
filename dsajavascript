Q.1) count the freq count of each character
let data = "my name is satyendra and i am a full stack developer"
let freq={}
for(char of data){
    if(freq[char])
    {
        freq[char]=freq[char]+1;
    }
    else{
        freq[char]=1;
    }
}
console.log(freq)

Q.2) suppose karo ki mujhe '':10 jo aa rha hai iski freq nahi btani hai to mai simply code ko modified kar sakta hu kuch is tarike se

let data = "my name is satyendra and i am a full stack developer"
let freq={}
for(char of data){
    if(char=' '){
        continue;
      }
    else if(freq[char])
    {
        freq[char]=freq[char]+1;
    }
    else{
        freq[char]=1;
    }
}
console.log(freq)

Q.3) count the each vowels freq count
let data = "my name is satyendra and i am a full stack developer"

vowels=['a','e','i','o','u']
let freq={}
for(char of data){
    if(!vowels.includes(char)){
        continue;
    }
    else if(freq[char])
    {
        freq[char]=freq[char]+1;
    }
    else{
        freq[char]=1;
    }
}
console.log(freq)

Q.4) count the each consonant freq count
let data = "my name is satyendra and i am a full stack developer"

vowels=['a','e','i','o','u']
let freq={}
for(char of data){
    if(vowels.includes(char)){
        continue;
    }
    else if(freq[char])
    {
        freq[char]=freq[char]+1;
    }
    else{
        freq[char]=1;
    }
}
console.log(freq)

